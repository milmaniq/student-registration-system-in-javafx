<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.srs.controller.BatchController">
   <children>
      <JFXTextField fx:id="txtSearchBatch" layoutX="217.0" layoutY="74.0" onInputMethodTextChanged="#txtSearchBatchOnTextChanged" promptText="Search Batch">
         <font>
            <Font size="13.0" />
         </font></JFXTextField>
      <Label layoutX="259.0" layoutY="33.0" text="Batches">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tblBatch" layoutX="14.0" layoutY="115.0" prefHeight="223.0" prefWidth="575.0">
        <columns>
          <TableColumn prefWidth="106.0" text="Batch Name" />
          <TableColumn prefWidth="96.0" text="Fee" />
            <TableColumn prefWidth="107.0" text="Start Date" />
            <TableColumn text="End Date" />
            <TableColumn prefWidth="81.0" text="Edit" />
            <TableColumn prefWidth="104.0" text="Remove" />
        </columns>
      </TableView>
      <ImageView fx:id="btnAddBatch" fitHeight="45.0" fitWidth="47.0" layoutX="542.0" layoutY="345.0" onMouseClicked="#btnAddBatchMouseClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icons8_Add_New_48px.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnBack" fitHeight="45.0" fitWidth="45.0" layoutX="8.0" layoutY="10.0" onMouseClicked="#btnBackMouseClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/icons8_Go_Back_48px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
